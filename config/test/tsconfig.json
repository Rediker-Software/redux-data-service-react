{
  "compilerOptions": {
    "target": "ES5",
    "jsx": "React",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "removeComments": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "outDir": "../../test-dist",
    "skipLibCheck": true,
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "include": [
    "../../types"
  ]
}
